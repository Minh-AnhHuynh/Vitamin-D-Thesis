author = {Jetter, Alexander and Egli, Andreas and Dawson-Hughes, Bess and Staehelin, Hannes B and Stoecklin, Elisabeth and Goessl, Richard and Henschkowski, Jana and Bischoff-Ferrari, Heike A}, 
